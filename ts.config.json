{
    "compilerOptions": {
      "target": "esnext",                 
      "module": "commonjs",               
      "jsx": "react-native",
      "moduleResolution": "node",              
      "esModuleInterop": true,             
      "allowJs": true,                    
      "skipLibCheck": true,                
      "forceConsistentCasingInFileNames": true  
    },
    "include": ["src/**/*"],               
    "exclude": ["node_modules"]           
  }
  